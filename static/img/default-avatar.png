nded-[18px] px-4 py-1.5 data-[multiline]:py-3" data-multiline=""><div class="whitespace-pre-wrap">{# forum/templates/accounts/profile_edit.html #}
{% extends 'base.html' %}
{% load forum_tags static %}

{% block content %}
&lt;div class="max-w-2xl mx-auto px-4 py-10"&gt;
  &lt;div class="bg-white/80 backdrop-blur-sm rounded-xl border border-gray-200/50 shadow p-8 space-y-8"&gt;

    &lt;h2 class="text-3xl font-bold text-center text-purple-800"&gt;
      🎨 Customize Your &lt;span class="text-purple-600"&gt;Profile&lt;/span&gt;
    &lt;/h2&gt;

    {# Genel form hataları #}
    {% if u_form.non_field_errors or p_form.non_field_errors %}
      &lt;ul class="space-y-2"&gt;
        {% for err in u_form.non_field_errors %}
          &lt;li class="bg-red-100 text-red-700 p-3 rounded"&gt;{{ err }}&lt;/li&gt;
        {% endfor %}
        {% for err in p_form.non_field_errors %}
          &lt;li class="bg-red-100 text-red-700 p-3 rounded"&gt;{{ err }}&lt;/li&gt;
        {% endfor %}
      &lt;/ul&gt;
    {% endif %}

    &lt;form method="post" enctype="multipart/form-data" action="{% url 'accounts:profile_edit' %}" class="space-y-6"&gt;
      {% csrf_token %}

      &lt;!-- 🔐 Account Info --&gt;
      &lt;div class="space-y-4"&gt;
        &lt;h3 class="text-xl font-semibold text-blue-700"&gt;🔐 Account Info&lt;/h3&gt;

        &lt;div class="space-y-1"&gt;
          {{ u_form.username|label_with_icon:"👤" }}
          {{ u_form.username|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Enter your username" |set_autofocus |set_required }}
        &lt;/div&gt;

        &lt;div class="space-y-1"&gt;
          {{ u_form.email|label_with_icon:"📧" }}
          {{ u_form.email|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Your email" |set_required }}
        &lt;/div&gt;

        &lt;div class="grid grid-cols-1 sm:grid-cols-2 gap-4"&gt;
          &lt;div class="space-y-1"&gt;
            {{ u_form.first_name|label_with_icon:"🪪" }}
            {{ u_form.first_name|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"First name" }}
          &lt;/div&gt;
          &lt;div class="space-y-1"&gt;
            {{ u_form.last_name|label_with_icon:"🧾" }}
            {{ u_form.last_name|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Last name" }}
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;

      &lt;!-- 🖼️ Profile Details --&gt;
      &lt;div class="space-y-4"&gt;
        &lt;h3 class="text-xl font-semibold text-purple-700"&gt;🖼️ Profile Details&lt;/h3&gt;

        &lt;div class="flex items-center gap-6"&gt;
          &lt;div class="relative"&gt;
            &lt;img id="avatarPreview"
                 src="{% if profile_picture_url %}{{ profile_picture_url }}{% else %}{% static 'img/default-avatar.png' %}{% endif %}"
                 alt="Avatar Preview"
                 class="w-24 h-24 rounded-full object-cover border border-gray-300 shadow-sm"&gt;
          &lt;/div&gt;
          &lt;div class="flex-1 space-y-1"&gt;
            {{ p_form.profile_picture|label_with_icon:"📷 Profile Picture" }}
            {{ p_form.profile_picture|add_class:"w-full" }}
          &lt;/div&gt;
        &lt;/div&gt;

        &lt;div class="space-y-1"&gt;
          {{ p_form.bio|label_with_icon:"📝" }}
          {{ p_form.bio|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Tell us a bit about yourself..." }}
        &lt;/div&gt;

        &lt;div class="space-y-1"&gt;
          {{ p_form.location|label_with_icon:"📍" }}
          {{ p_form.location|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Your location" }}
        &lt;/div&gt;

        &lt;div class="space-y-1"&gt;
          {{ p_form.website|label_with_icon:"🌐" }}
          {{ p_form.website|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Website URL" }}
        &lt;/div&gt;

        &lt;div class="space-y-1"&gt;
          {{ p_form.twitter|label_with_icon:"🐦" }}
          {{ p_form.twitter|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"Twitter URL" }}
        &lt;/div&gt;

        &lt;div class="space-y-1"&gt;
          {{ p_form.linkedin|label_with_icon:"🔗" }}
          {{ p_form.linkedin|add_class:"w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-purple-500" |add_placeholder:"LinkedIn URL" }}
        &lt;/div&gt;
      &lt;/div&gt;

      &lt;!-- 💾 Submit --&gt;
      &lt;div class="text-center"&gt;
        &lt;button type="submit"
                class="bg-green-600 hover:bg-green-700 text-white font-semibold px-6 py-3 rounded-lg transition"&gt;
          💾 Save Changes
        &lt;/button&gt;
        &lt;p class="mt-4 text-sm"&gt;
          &lt;a href="{% url 'accounts:user_profile' username=request.user.username %}"
             class="text-blue-600 hover:underline"&gt;
            ← Back to Profile
          &lt;/a&gt;
        &lt;/p&gt;
      &lt;/div&gt;
    &lt;/form&gt;
  &lt;/div&gt;
&lt;/div&gt;

&lt;script&gt;
  const input = document.querySelector('input[type="file"][name$="profile_picture"]');
  const preview = document.getElementById('avatarPreview');
  if (input) {
    input.addEventListener('change', function () {
      const [file] = input.files;
      if (file) {
        preview.src = URL.createObjectURL(file);
      }
    });
  }
&lt;/script&gt;
{% endblock %}</div></div></div></div></div><div class="mx-[var(--mini-thread-content-inset)]"><div class="flex justify-end"><div class="touch:-me-2 touch:-ms-3.5 -ms-2.5 -me-1 flex flex-wrap items-center gap-y-4 p-1 select-none focus-within:transition-none hover:transition-none duration-300 group-hover/turn-messages:delay-300 pointer-events-none opacity-0 motion-safe:transition-opacity group-hover/turn-messages:pointer-events-auto group-hover/turn-messages:opacity-100 group-focus-within/turn-messages:pointer-events-auto group-focus-within/turn-messages:opacity-100 has-data-[state=open]:pointer-events-auto has-data-[state=open]:opacity-100" style="mask-position: 0% 0%;"><button class="text-token-text-secondary hover:bg-token-bg-secondary rounded-lg" aria-label="Kopyala" aria-selected="false" data-testid="copy-turn-action-button" data-state="closed"><span class="touch:w-10 flex h-8 w-8 items-center justify-center"><svg width="20" height="20" viewBox="0 0 20 20" fill="currentColor" xmlns="http://www.w3.org/2000/svg" class="icon"><path d="M12.668 10.667C12.668 9.95614 12.668 9.46258 12.6367 9.0791C12.6137 8.79732 12.5758 8.60761 12.5244 8.46387L12.4688 8.33399C12.3148 8.03193 12.0803 7.77885 11.793 7.60254L11.666 7.53125C11.508 7.45087 11.2963 7.39395 10.9209 7.36328C10.5374 7.33197 10.0439 7.33203 9.33301 7.33203H6.5C5.78896 7.33203 5.29563 7.33195 4.91211 7.36328C4.63016 7.38632 4.44065 7.42413 4.29688 7.47559L4.16699 7.53125C3.86488 7.68518 3.61186 7.9196 3.43555 8.20703L3.36524 8.33399C3.28478 8.49198 3.22795 8.70352 3.19727 9.0791C3.16595 9.46259 3.16504 9.95611 3.16504 10.667V13.5C3.16504 14.211 3.16593 14.7044 3.19727 15.0879C3.22797 15.4636 3.28473 15.675 3.36524 15.833L3.43555 15.959C3.61186 16.2466 3.86474 16.4807 4.16699 16.6348L4.29688 16.6914C4.44063 16.7428 4.63025 16.7797 4.91211 16.8027C5.29563 16.8341 5.78896 16.835 6.5 16.835H9.33301C10.0439 16.835 10.5374 16.8341 10.9209 16.8027C11.2965 16.772 11.508 16.7152 11.666 16.6348L11.793 16.5645C12.0804 16.